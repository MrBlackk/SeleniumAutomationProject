{
    "class": "org.openqa.grid.common.RegistrationRequest",
    "capabilities": [
        {
            "seleniumProtocol": "WebDriver",
            "browserName": "firefox",
            "version": "30.0",
            "maxInstances": 1,
            "platform" : "WINDOWS"
        }
    ],
    "configuration":{
            "port": 8989,
            "register": true,
            "host": "localhost",
            "proxy": "org.openqa.grid.selenium.proxy.DefaultRemoteProxy",
            "maxSession": 2,
            "hubHost": "localHost",
            "role": "webdriver",
            "registerCycle": 5000,
            "hub": "http://localhost:4444/grid/console",
            "remoteHost": "http://localhost:8989"
    }
}